} 
QLineEdit:focus{ 
	border-image: url(qss/opencoffee/lineEditBg_focus.png) 4 4 4 4 stretch; 
	border-width: 4px 4px 4px 4px; 
	border-radius: 0px;
}
QSlider:horizontal {
	width: 100px;
	min-width: 100px;
}

QSlider::handle:horizontal {
     background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 transparent, stop:1 white);
     width: 10px;
     margin: -2px 0; 
     border-radius: 3px;
 }
QSlider::groove:horizontal{
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                 stop:0 rgb(200,200,200), stop:1 transparent);
}
QListWidget { 
	border: 0px; 
