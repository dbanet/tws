	border:0px;
}
#chatwindow QScrollBar::up-arrow:vertical,#chatwindow QScrollBar::down-arrow:vertical,#channelwindow QScrollBar::up-arrow:vertical,#channelwindow QScrollBar::down-arrow:vertical,#channelwindow  QScrollBar::left-arrow:horizontal, #channelwindow QScrollBar::right-arrow:horizontal{
border-image:none;
}
/**--------------------------------------------------------------------------specific**/
#channelwindow QScrollBar:horizontal {
border-image:none;
     background: transparent;
     margin: 0px 20px 0px 20px;
 }
#chatwindow QScrollBar:vertical, #channelwindow QScrollBar:vertical {
	border-image:none;
     background: transparent;
     margin: 20px 0px 20px 0px;
 }
#channelwindow QScrollBar::handle:horizontal {
	min-width:20px;
     border-image: url(qss/quake4/scrollbarhandlehor.svg) 0 0 0 0;
 }
#chatwindow QScrollBar::handle:vertical, #channelwindow QScrollBar::handle:vertical {
	min-height:20px;
     border-image: url(qss/quake4/scrollbarhandlevert.svg) 0 0 0 0;
 }
#channelwindow QScrollBar::right-arrow:horizontal {
	border-image: url(qss/quake4/rightarrow.svg);	
     width: 20px;
    height: 20px;
     subcontrol-position: right;
     subcontrol-origin: margin;
 }
#chatwindow QScrollBar::add-line:vertical, #channelwindow QScrollBar::add-line:vertical {
	border-image: url(qss/quake4/downarrow.svg);
     height: 20px;
     width: 20px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }

#channelwindow QScrollBar::sub-line:horizontal {
     border-image: url(qss/quake4/leftarrow.svg);
     width: 20px;
     height: 20px;
     subcontrol-position: left;
     subcontrol-origin: margin;
 }
#chatwindow QScrollBar::sub-line:vertical, #channelwindow QScrollBar::sub-line:vertical {
     border-image: url(qss/quake4/uparrow.svg);
     height: 20px;
     width: 20px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }
#channelwindow QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
     background: transparent;
}
#channelwindow QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: transparent;
}
#chatwindow QPushButton,#channelwindow QPushButton{
border-image:none;
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 rgba(186,201,62,20%), stop:1 black);
	height: 20;
border:0px;
}
#chatwindow QPushButton:hover, #channelwindow QPushButton:hover{
border-image:none;
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 rgba(186,201,62,20%), stop:1 rgba(186,201,62,20%));
}
#chatwindow QPushButton:pressed, #channelwindow QPushButton:pressed {
border-image:none;
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 rgba(86,201,62,50%), stop:1 rgba(86,201,62,50%));
	height: 20;
}
#chatwindow QPushButton,#chatwindow QPushButton:pressed, #chatwindow QPushButton:hover, #channelwindow QPushButton, #channelwindow QPushButton:pressed, #channelwindow QPushButton:hover{
border-image:none;
	color: rgb(100,100,255);
	font: bold;
	font-family: "Yudit";
}

