/**-----------------------------------------------------------------------CHANNELWINDOW**/
#channelwindow QScrollBar:horizontal {
     background-color: gray;
     height: 10px;
     margin: 0px 10px 0px 10px;
 }
#channelwindow QScrollBar:vertical {
     background-color: grey;
     width: 10px;
     margin: 10px 0px 10px 0px;
 }
#channelwindow QScrollBar::handle{
	border-image:none;
    	border:1px solid black;
	background:rgb(254,182,168);
}
#channelwindow QScrollBar::handle:horizontal {
     	min-width: 20px;
     	height: 20px;
 }
#channelwindow QScrollBar::handle:vertical {
     	min-height: 50px;
     	width: 20px;
 }
#channelwindow QScrollBar::add-line:horizontal {
     border: 1px solid grey;
     background: black;
     width: 10px;
     subcontrol-position: right;
     subcontrol-origin: margin;
 }
#channelwindow QScrollBar::add-line:vertical {
     border: 1px solid grey;
     background: black;
     height: 10px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }

#channelwindow QScrollBar::sub-line:horizontal {
     border: 1px solid grey;
     background: black;
     width: 10px;
     subcontrol-position: left;
     subcontrol-origin: margin;
 }
#channelwindow QScrollBar::sub-line:vertical {
     border: 1px solid grey;
     background: black;
     height: 10px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }
#channelwindow QScrollBar::left-arrow:horizontal{
background-image:none;
     image: url(qss/wormnet/leftarrow.png);
 }
#channelwindow QScrollBar::right-arrow:horizontal {
background-image:none;
     image: url(qss/wormnet/rightarrow.png);
 }
#channelwindow QScrollBar::up-arrow:vertical {
background-image:none;
	image: url(qss/wormnet/uparrow.png);
 }
#channelwindow QScrollBar::down-arrow:vertical {
background-image:none;
	image: url(qss/wormnet/downarrow.png);
 }

#channelwindow QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
     background: gray;
 }
#channelwindow QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: gray;
 }
#channelwindow QTreeView{
	alternate-background-color:transparent;
}
#channelwindow QTreeView::item:selected { 
	background-color: red;
}
#channelwindow QTreeView#hosts{
	border: 0px;
	padding: 2px 1px 2px 1px;
	background-color: transparent;
	color: rgb(255,191,191);
	font: "Adobe New Century Schoolbook" bold;
	alternate-background-color: transparent;
}
#channelwindow QTreeView#users{
	border: 0px;
	padding: 2px 1px 2px 1px;
	background-color: transparent;
	color: white;
	font: "Adobe New Century Schoolbook" bold;
	alternate-background-color: transparent;
}
#channelwindow QSplitter::handle {
     background-color: gray;
     height: 2px;
     width: 2px;		
}
#channelwindow QSplitter::handle:hover {
     background-color: red;
     height: 2px;
     width: 2px;		
}
#channelwindow QHeaderView::section {
     background-color: black;
     color: white;
     padding-left: 4px;
     border: 1px solid #6c6c6c;
}
#channelwindow QPushButton{
	border-image:none;
	border:2px solid gray;
	background-color:black;
	color: white;
}
#channelwindow QPushButton:hover{
	border:2px solid white;
	background:black;
}
#channelwindow QPushButton:pressed {
	border:2px solid yellow;
	background: rgb(0,0,64);
}
#channelwindow QLineEdit{ 
	border-image:none;
	border:2px solid gray;
	background:black;
	color: white;
} 
#channelwindow QLineEdit:hover{
	border:2px solid white;
	background:black;
	border-radius: 0px;
} 
#channelwindow QLineEdit:focus{ 
	border:2px solid yellow;
	background:transparent;
	border-radius: 0px;
}
#channelwindow QTextEdit:focus{
	border:0px;
}
